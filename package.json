{
  "name": "style-logger",
  "version": "1.1.0",
  "description": "logs to stdout or a stream optionaly using styles passed by the consumer",
  "main": "stylelogger.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "build": "browserify ./test/test-browserify.js -o ./test/bundle.js -t [ babelify --presets [ es2015 ] ]"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "log",
    "stdout",
    "stream",
    "ANSI escape sequences"
  ],
  "author": "cool.blue@y7mail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babelify": "^7.3.0",
    "chai": "^1.10.0",
    "cross-env": "^2.0.0",
    "intercept-stdout": "^0.1.2",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cool-Blue/style-logger.git"
  },
  "bugs": {
    "url": "https://github.com/cool-Blue/style-logger/issues"
  },
  "homepage": "https://github.com/cool-Blue/style-logger#readme",
  "dependencies": {
    "chalk": "^1.1.3"
  }
}
